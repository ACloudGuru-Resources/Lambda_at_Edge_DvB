{
  "name": "acg-lambda-edge",
  "version": "0.1.0",
  "private": true,
  "eslintConfig": {
    "env": {
      "browser": false,
      "commonjs": true,
      "node": true,
      "es6": true
    },
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parser": "babel-eslint",
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "all"
        }
      ]
    }
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "serverless-s3-sync": "^1.6.1"
  }
}
